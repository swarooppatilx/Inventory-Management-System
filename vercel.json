{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" },
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  